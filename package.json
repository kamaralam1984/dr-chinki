    "@google/genai": "1.3.0",
    "@react-three/drei": "10.0.0",
    "@react-three/fiber": "9.0.0",
    "docx": "9.1.0",
    "jspdf": "2.5.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "three": "0.171.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
